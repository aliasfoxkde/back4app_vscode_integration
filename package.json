{
  "name": "back4app-vscode-integration",
  "displayName": "Back4App Integration",
  "description": "A VSCode extension for integrating Back4App functionalities",
  "version": "0.0.1",
  "publisher": "aliasfoxkde",
  "engines": {
    "vscode": "^1.55.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:back4appIntegration.helloWorld"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "back4appIntegration.helloWorld",
        "title": "Hello World"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run package",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile",
    "test": "node ./out/test/runTest.js",
    "lint": "eslint src --ext ts"
  },
  "devDependencies": {
    "@types/node": "^14.18.63",
    "@types/vscode": "^1.92.0",
    "@typescript-eslint/eslint-plugin": "^4.33.0",
    "@typescript-eslint/parser": "^4.33.0",
    "eslint": "^7.32.0",
    "typescript": "^4.9.5",
    "vscode-test": "^1.4.0"
  },
  "keywords": [],
  "author": "",
  "license": "ISC"
}
